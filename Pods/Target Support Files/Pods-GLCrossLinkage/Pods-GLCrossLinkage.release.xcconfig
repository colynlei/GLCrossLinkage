GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/GLSegmentedControl" "${PODS_ROOT}/Headers/Public/MJRefresh"
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/GLSegmentedControl" "${PODS_CONFIGURATION_BUILD_DIR}/MJRefresh"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/GLSegmentedControl" -isystem "${PODS_ROOT}/Headers/Public/MJRefresh"
OTHER_LDFLAGS = $(inherited) -ObjC -l"GLSegmentedControl" -l"MJRefresh"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
